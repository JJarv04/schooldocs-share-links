{
    "manifest_version": 3,
    "name": "SchoolDocs Share Links",
    "description": "Allows you to quickly create sharelinks for SchoolDocs documents with your school guid.",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html",
        "default_title": "SchoolDocs Share Links"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}